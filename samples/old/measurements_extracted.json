  "bleed_calculations": [
    {
      "id": "monthly_delayed_orders",
      "label": "Monthly Delayed Orders",
      "formula": "volume × (1 - (1 - overselling_rate))",
      "inputs": [
        "volume",
        "overselling_rate"
      ],
      "result": 224,
      "result_display": "224"
    },
    {
      "id": "monthly_lost_sales",
      "label": "Monthly Lost Sales",
      "formula": "volume × lost_sales_rate",
      "inputs": [
        "volume",
        "lost_sales_rate"
      ],
      "result": 312,
      "result_display": "312"
    },
    {
      "id": "monthly_bleed",
      "label": "Monthly Bleed",
      "formula": "volume × (1 - (1 - overselling_rate)) × cost_per_delayed_order + volume × lost_sales_rate × cost_per_order",
      "inputs": [
        "volume",
        "overselling_rate",
        "cost_per_delayed_order",
        "lost_sales_rate",
        "cost_per_order"
      ],
      "result": 4050,
      "result_display": "$4,050"
    }
  ],
